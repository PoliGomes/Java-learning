
public class CriaConta {
	public static void main(String[] args) {
		Conta primeiraConta = new Conta();
		
		System.out.println(primeiraConta.getSaldo());
		
		System.out.println(primeiraConta.getSaldo());
		
		Conta segundaConta = new Conta();

		System.out.println(primeiraConta.getSaldo());
		System.out.println(segundaConta.getSaldo());
	}
}
