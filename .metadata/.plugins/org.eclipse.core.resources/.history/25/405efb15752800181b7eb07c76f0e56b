
public class TestarVariaveis {
	public static void main(String [] args){
		System.out.println("olá novo teste");
		int idade = 30;
		System.out.println(idade);
		System.out.println("Minha idade é: " + idade + " anos");
		double peso = 4.0;
		int quantidade = 10;
		System.out.println(peso * quantidade);
		
		double valor = 12.5;
		
		
		System.out.println(valor);
	}
}
